<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <color name="bottomNav">#000000</color>
    <style name="Theme.Keyo" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#EDEDED</item>
        <item name="colorPrimaryVariant">#121212</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#b883fe</item>
        <item name="colorSecondaryVariant">#b883fe</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:navigationBarColor">@color/black</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.SplashTheme" parent="Theme.SplashScreen">
        <item name="windowSplashScreenAnimatedIcon">@drawable/splash_light_logo</item>
        <item name="windowSplashScreenBackground">@color/black</item>
        <item name="postSplashScreenTheme">@style/Theme.Keyo</item>
    </style>
</resources>